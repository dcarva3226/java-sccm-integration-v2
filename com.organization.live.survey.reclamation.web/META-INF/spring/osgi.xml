<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:osgi="http://www.springframework.org/schema/osgi"
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
                        http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd
                        http://www.springframework.org/schema/osgi http://www.springframework.org/schema/osgi/spring-osgi.xsd">
	
    <osgi:reference id="authService" interface="com.organization.live.auth.AuthService" />
	
	<osgi:service ref="serviceInitJobBuilder" interface="com.organization.live.scheduler.JobBuilder" />
	
	<osgi:reference interface="com.organization.live.scheduler.SchedulerService" id="schedulerService"/>
	
	<osgi:service ref="reclamationServiceInitJobInstaller" interface="com.organization.live.plugin.Installable" />
	
	<osgi:reference id="loggerFactory" interface="com.organization.live.logger.LoggerFactory"/>
	
</beans>